{
  "webroot": "Public",
  "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Diagnostics": "1.0.0-beta5-12103",
    "Microsoft.AspNet.Mvc": "6.0.0-beta5-12103",
    "Microsoft.AspNet.Mvc.TagHelpers": "6.0.0-beta5-12103",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta5-12103",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-beta5-12103",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta5-12103",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta5-12103",
    "Microsoft.AspNet.Tooling.Razor": "1.0.0-beta5-12103",
    "Microsoft.Framework.Configuration.Json": "1.0.0-beta5-12103",
    "Microsoft.Framework.Logging": "1.0.0-beta5-12103",
    "Microsoft.Framework.Logging.Console": "1.0.0-beta5-12103",
    "Kestrel": "1.0.0-beta5-12103"
  },

  "commands": {
    "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --config hosting.ini",
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --config hosting.ini"
  },

  "frameworks": {
    "dnxcore50": {}
  },

  "exclude": [
    "Public",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "Content",
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "scripts": {
    "prepublish": [
      "npm install",
      "bower install",
      "gulp clean compile min"
    ]
  }
}
